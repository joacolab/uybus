@model IEnumerable<Share.entities.EViaje>

@{
    ViewBag.Title = "selecSalida";
}

<h2>Seleccione fecha del viaje</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Fecha)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Convert.ToString(string.Format("{0:dd/MM/yyyy}", item.Fecha))
        </td>
        <td>
            @Html.ActionLink("Seleccionar", "selecViaje", new {  id=item.IdViaje })
        </td>
    </tr>
}

</table>
