@model IEnumerable<Share.entities.EViaje>

@{
    ViewBag.Title = "traerViajes";
}

<h2>Viajes</h2>

<p>
    @Html.ActionLink("Crear viajes", "crearViaje")
</p>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.IdViaje)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Finalizado)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Fecha)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.HoraInicioReal)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IdSalida)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.IdViaje)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Finalizado)
            </td>
            <td>
                @Convert.ToString(string.Format("{0:dd/MM/yyyy}", item.Fecha))

            </td>
            <td>
                @Html.DisplayFor(modelItem => item.HoraInicioReal)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdSalida)
            </td>
            <td>
                @Html.ActionLink("Editar", "editarViaje", new { id = item.IdViaje })
            </td>
        </tr>
    }

</table>

<div>
    @Html.ActionLink("ir al inicio", "index")
</div>
