@model IEnumerable<Share.entities.EParada>

@{
    ViewBag.Title = "traerParadaL";
}

<h2>Seleccione parada</h2>

<table class="table">
    <tr>
        <th>
            Identificador de Parada
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Nombre)
        </th>
        <th>
           Latitud
        </th>
        <th>
            Longitud
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.IdParada)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Nombre)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Lat)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Long)
        </td>
        <td>
            @Html.ActionLink("Seleccionar", "asignarParada", new {  id=item.IdParada  }) 
        </td>
    </tr>
}

</table>

<div>
    @Html.ActionLink("Ir al inicio", "Index")
</div>