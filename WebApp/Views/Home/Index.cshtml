@model IEnumerable<Share.entities.EVehiculo>

@{
    ViewBag.Title = "Index";
}

<h2>Vehiculos</h2>

<p>
    @Html.ActionLink("Nuevo vehiculo", "nuevoVehiculo")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Matricula)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Modelo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Marca)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CantAsientos)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Matricula)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Modelo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Marca)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CantAsientos)
        </td>
        <td>
            @Html.ActionLink("Editar", "editarVehiculo", new {  id=item.Matricula  })
        </td>
    </tr>
}

</table>
